                  msg (o + " Current location: " + o.parent)
                }
                foreach (b, AllObjects()) {
                  if (b = GetObject(text)) {
                    // msg (b.name)
                    if (not b.name = null) {
                      game.attnames = GetAttributeNames(b, true)
                      msg ("<br><b>ChildObjects:</b>")
                      binv = (GetAllChildObjects(b))
                      foreach (obj, binv) {
                        msg (obj)
                      }
