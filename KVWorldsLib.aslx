      text = Split(text, ",")
      // msg (StringListItem(text, 0))
      t1 = (StringListItem(text, 0))
      // msg (StringListItem(text, 1))
      t2 = (StringListItem(Split((StringListItem(Split((StringListItem(text, 1)), " "), 1)), "\""), 1))
      // msg (t2)
      switch (t1) {
        case ("Hide") {
          request (Hide, t2)
          if (t2 = "Panes") {
            game.showpanes = false
          }
        }
        case ("Show") {
          request (Show, t2)
